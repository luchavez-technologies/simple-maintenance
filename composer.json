{
    "name": "luchavez/simple-maintenance",
    "description": "Maintenance logs for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "James Carlo Luchavez",
            "email": "jamescarloluchavez@gmail.com",
            "homepage": "https://www.linkedin.com/in/jsluchavez"
        }
    ],
    "homepage": "https://github.com/luchavez-technologies/simple-maintenance",
    "keywords": ["Laravel", "SimpleMaintenance"],
    "require": {
        "php": "^8.0",
        "illuminate/support": "~8|~9|~10",
        "luchavez/starter-kit": "^1.0",
        "spatie/laravel-tags": "^4.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~6|~7|~8"
    },
    "suggest": {
        "luchavez/starter-kit": "^1.0",
        "luchavez/boilerplate-generator": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Luchavez\\SimpleMaintenance\\": "src/",
            "Luchavez\\SimpleMaintenance\\Database\\Seeders\\": "database/seeders/",
            "Luchavez\\SimpleMaintenance\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luchavez\\SimpleMaintenance\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luchavez\\SimpleMaintenance\\Providers\\SimpleMaintenanceServiceProvider"
            ],
            "aliases": {
                "SimpleMaintenance": "Luchavez\\SimpleMaintenance\\Facades\\SimpleMaintenance"
            }
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.0.x-dev",
            "dev-feature": "1.0.x-dev"
        }
    }
}
